{
  "compilerOptions": {
    "jsx": "react-native",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["es6", "dom"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx"]
}